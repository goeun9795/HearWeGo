<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LogoActivity">

    <!-- 로고 -->
    <RelativeLayout
        android:id="@+id/logo"
        android:layout_width="411dp"
        android:layout_height="823dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="1038dp"
        android:background="@drawable/background"
        tools:ignore="MissingConstraints"
        android:layout_alignParentStart="true"
        android:layout_marginStart="216dp">

        <!-- Status Bar -->

        <!-- Frame_Logo -->
        <RelativeLayout
            android:id="@+id/container_logo"
            android:layout_width="149dp"
            android:layout_height="101dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="301dp"
            android:layout_alignParentStart="true"
            android:layout_marginStart="131dp"
            android:background="@drawable/hear_we_go" />

        <!-- Frame_google_login -->
        <com.google.android.gms.common.SignInButton
            android:id="@+id/btn_Google"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/container_logo"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="60dp"
            android:layout_marginLeft="60dp"
            android:layout_marginTop="120dp"
            android:layout_marginEnd="60dp"
            android:layout_marginRight="60dp"
            android:layout_marginBottom="230dp"
            android:background="@drawable/google_login" />

        <!--<ImageButton
            android:id="@+id/google_login"
            android:layout_width="395dp"
            android:layout_height="90dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="603dp"
            android:layout_marginBottom="130dp"
            android:background="@drawable/google_login"
            android:clickable="true"
            android:layout_marginStart="8dp"
            android:focusable="true"
            android:onClick="onLoginClicked"
            tools:ignore="MissingConstraints,OnClick,UsingOnClickInXml" />-->
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>